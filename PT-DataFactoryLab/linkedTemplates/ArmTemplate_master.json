{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "PT-DataFactoryLab"
		},
		"AWS_S3_Link_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AWS_S3_Link'"
		},
		"PT_ADLS_LabEnv_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'PT_ADLS_LabEnv'"
		},
		"PT_BLOB_LabEnv_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'PT_BLOB_LabEnv'"
		},
		"AWS_S3_Link_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAUAYPBUE3M75OUDNN"
		},
		"HTTP_Weather_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://weather-b362.restdb.io/rest/random-weather-dec-2016"
		},
		"Lego_RebrickableURL_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://cdn.rebrickable.com/media/downloads/"
		},
		"PT_ADLS_LabEnv_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ptadfdatalake.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AWS_S3_Link_secretAccessKey": {
						"value": "[parameters('AWS_S3_Link_secretAccessKey')]"
					},
					"PT_ADLS_LabEnv_accountKey": {
						"value": "[parameters('PT_ADLS_LabEnv_accountKey')]"
					},
					"PT_BLOB_LabEnv_connectionString": {
						"value": "[parameters('PT_BLOB_LabEnv_connectionString')]"
					},
					"AWS_S3_Link_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AWS_S3_Link_properties_typeProperties_accessKeyId')]"
					},
					"HTTP_Weather_properties_typeProperties_url": {
						"value": "[parameters('HTTP_Weather_properties_typeProperties_url')]"
					},
					"Lego_RebrickableURL_properties_typeProperties_url": {
						"value": "[parameters('Lego_RebrickableURL_properties_typeProperties_url')]"
					},
					"PT_ADLS_LabEnv_properties_typeProperties_url": {
						"value": "[parameters('PT_ADLS_LabEnv_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}